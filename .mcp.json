{
  "mcpServers": {
    "laravel-boost": {
      "type": "stdio",
      "command": "php",
      "args": [
        "artisan",
        "boost:mcp"
      ],
      "env": {}
    },
    "tailwindcss": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "tailwindcss-mcp-server"
      ],
      "env": {}
    }
  }
}